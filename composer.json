{
    "name": "wzj177/short-video-parse",
    "description": "短视频的拓展包，集成各大短视频的去水印功能",
    "type": "library",
    "keywords": [
        "video parse tools",
        "short video parse"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "wzj177",
            "email": "wzj177@163.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "ext-openssl": "*",
        "ext-SimpleXML": "*",
        "ext-mbstring": "*",
        "ext-libxml": "*",
        "guzzlehttp/guzzle": "^7.3|^6.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.2"
    },
    "autoload": {
        "psr-4": {
            "Wzj\\ShortVideoParse\\": "src"
        },
        "files": [
            "config/url-validator.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Wzj\\Tests\\": "tests"
        }
    }
}
